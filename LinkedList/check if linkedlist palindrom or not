   public boolean isPalindrome(ListNode head) {
        Stack<Integer> s = new Stack<Integer>();
        ListNode temp = head;
        while(temp!= null){
            s.push(temp.val);
            temp = temp.next;
        }
        temp =head;
        while(temp!=null){
           
            if(temp.val != s.peek())
            return false;
        
                s.pop();
                temp = temp.next;
            
        }
        return true;
        
    }
}
