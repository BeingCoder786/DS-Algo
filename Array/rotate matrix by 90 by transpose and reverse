class Solution {
    fun rotate(matrix: Array<IntArray>): Unit {
         
          val n = matrix.size
          for(i in 0 until n){
            for(j in i until n){
                val temp = matrix[i][j]
                matrix[i][j] =matrix[j][i]
                matrix[j][i] = temp
            }
          }

          for(i in 0 until n){
            matrix[i].reverse()
          }
          
    }
}
